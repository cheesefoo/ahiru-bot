{
    "name": "ahiru-bot",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": "16.6.0"
    },
    "main": "dist/start.js",
    "scripts": {
        "testlol": "node ./dist/utils/twitter-space-utils.js",
        "postinstall": "tsc",
        "lint": "tslint --project .",
        "lint:fix": "tslint --fix --project .",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "build": "tsc --project tsconfig.json",
        "register": "npm run build && node --enable-source-maps dist/start.js --register",
        "start": "npm run build && node --enable-source-maps dist/start.js",
        "start:dev": "ts-node-dev --watch src/**/*.ts,config/**/*.json,lang/**/*.json src/start.ts",
        "start:shard": "npm run build && node --enable-source-maps dist/app.js",
        "start:pm2": "npm run build && npm run pm2:start",
        "pm2:start": "pm2 start process.json",
        "pm2:stop": "pm2 stop process.json",
        "pm2:delete": "pm2 delete process.json"
    },
    "dependencies": {
        "@discordjs/rest": "^0.1.0-canary.0",
        "@google-cloud/translate": "^6.3.1",
        "@google-cloud/vision": "^2.4.0",
        "body-parser": "^1.19.1",
        "canvas": "^2.8.0",
        "class-transformer": "0.4.0",
        "class-validator": "0.13.1",
        "deepl": "^1.0.13",
        "discord-api-types": "^0.23.1",
        "discord.js": "13.2.0",
        "discord.js-rate-limiter": "1.2.3",
        "express": "4.17.1",
        "express-promise-router": "4.1.0",
        "filesize": "8.0.3",
        "form-data": "^4.0.0",
        "holodex.js": "^1.1.0",
        "linguini": "1.2.1",
        "node-fetch": "2.6.5",
        "node-html-parser": "^5.0.0",
        "node-schedule": "2.0.0",
        "node-worker-threads-pool": "^1.5.1",
        "pg": "^8.7.1",
        "pino": "6.13.3",
        "pino-pretty": "7.0.1",
        "pm2": "^5.1.2",
        "reflect-metadata": "^0.1.13",
        "remove-markdown": "0.3.0",
        "sanitize-filename": "^1.6.3",
        "socket.io": "^4.4.1",
        "twitter-v2": "^1.1.0",
        "typescript": "^4.4.4",
        "unofficial-jisho-api": "^2.2.2"
    },
    "devDependencies": {
        "@types/express": "4.17.13",
        "@types/node": "16.10.3",
        "@types/node-fetch": "2.5.12",
        "@types/node-schedule": "1.3.2",
        "@types/pg": "^8.6.1",
        "@types/pino": "6.3.11",
        "@types/remove-markdown": "0.3.1",
        "eslint": "^8.1.0",
        "prettier": "^2.4.1",
        "ts-node-dev": "^1.1.8"
    }
}
